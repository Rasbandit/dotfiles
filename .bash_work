#!/bin/bash

alias edit_work="code ~/.bash_work"

function cd_work() {
    mkdir -p ~/Documents/work
    cd ~/Documents/work
}

alias cdw='cd_work'

alias tsu='sudo tailscale up --accept-routes --exit-node=100.115.207.254 --exit-node-allow-lan-access=true'
alias tsd='sudo tailscale down'
alias tss='tailscale status'
